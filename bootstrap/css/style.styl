.navbar
	a
		img
			margin-top -10px
	background-color #1e2b50
	border-radius 0px
	margin-bottom 0px
	min-height 110px
.navbar-header
	button
		margin-top 30px
.customnavbar
	ul
		li
			a
				color #fff !important
				text-transform uppercase
				-webkit-transition: all .3s ease-in-out;
				-moz-transition: all .3s ease-in-out;
				-ms-transition: all .3s ease-in-out;
				-o-transition: all .3s ease-in-out;
				-transition: all .3s ease-in-out;
				&:hover 
					background-color #fff !important
					color #f00 !important
					-webkit-opacity: 1;
					-moz-opacity: 1;
					opacity: 1;
	.navbar-right
		margin-top 30px
		.input-group
			a
				color #ccc !important
				&:hover
					background-color #ccc !important
					color #333 !important

#input-src
	display none
	max-width 200px
	width 0px
#div.src
	display none
	margin-top 8px
	margin-left 4px
#btn-src 
	display none
.homebox
	margin-top 16px
	width 100%
	.icon
		display inline-block
		width 15%
		margin-right 5%
		img 
			width 100%
	.desc
		float left
		width 80%
		
		.title
			display block
			font-size 1.6em
			font-weight normal
			margin-bottom 6px
		p
			color #555
			font-size 0.9
			text-align justify;
.bar-top
	background-color #001931
	ul
		display inline-block
		list-style-type none
		color #f3f3f3 !important
		margin 0px
		li
			float left
			color #fff !important
			display inline-block
			a 
				color #fff !important
				display block
				padding 8px
				-webkit-transition: all .3s ease-in-out;
				-moz-transition: all .3s ease-in-out;
				-ms-transition: all .3s ease-in-out;
				-o-transition: all .3s ease-in-out;
				-transition: all .3s ease-in-out;
				&:hover 
					-webkit-opacity: 1;
					-moz-opacity: 1;
					opacity: 1;
			.facebook
				&:hover
					background-color #3b5898
			.twitter
				&:hover
					background-color #2185B2
			.youtube
				&:hover
					background-color #CC171D
